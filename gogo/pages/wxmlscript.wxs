//#1=>1,去除#字符
var iscontain = function(arraylist,item){
  item = item + "";
  for(var i=0;i<arraylist.length;i++){
    if(arraylist[i] == item){
      return true;
    }
  }
  return false;
}
var show_member_num = function (member_num_str) {
  member_num_str = ""+member_num_str;
  return member_num_str.replace("#","");
}
//scores最后一位是比分标签
var show_vs_scores = function(scores){
  var flag = true;
  //console.log(scores);
  for(var i=0;i<scores.length;i++){
    if(scores[i]!=0){
      flag = false;
      break;
    }
  }
  if(flag){
    return "未开始";
  }else {
    return scores.join(" : ");
  }
}

var not_equal = function(v1,v2,v3){
  if(v1 != v2 || v1 != v3){
    return true;
  }else{
    return false;
  }
}

module.exports = {
  show_member_num: show_member_num,
  show_vs_scores:show_vs_scores,
  iscontain:iscontain,
  not_equal:not_equal
};
