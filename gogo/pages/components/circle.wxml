<!-- components/circle/circle.wxml -->
<view class="circle_box" style="width:{{size}}px;height:{{size}}px;">
      <canvas class="circle_bg" type="2d" id="{{draw}}bg" canvas-id="{{draw}}bg" style="width:{{size}}px;height:{{size}}px"></canvas> 
      <canvas class="circle_draw" type="2d" id="{{draw}}" canvas-id="{{draw}}" style="width:{{size}}px;height:{{size}}px"></canvas> 
      
      <text class='circle_txt'> {{content}} </text>  
</view>